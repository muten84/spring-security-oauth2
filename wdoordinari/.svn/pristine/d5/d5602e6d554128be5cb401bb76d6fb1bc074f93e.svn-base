<div class="input-group spinner">


  <div class="input-group-btn-vertical">
    <button class="btn btn-primary" type="button" (click)="spinnerUp()">
      <i class="fa fa-caret-up"></i>
    </button>
    <button class="btn btn-primary" type="button" (click)="spinnerDown()">
      <i class="fa fa-caret-down"></i>
    </button>
  </div>

</div>
