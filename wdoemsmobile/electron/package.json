{
    "name": "terminal-electron",
    "productName": "terminal-electron",
    "version": "1.0.0",
    "description": "My Electron application description",
    "main": "src/start.js",
    "scripts": {
        "start-electron": "electron-forge start",
        "start": "npm run wrapper",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint src --color",
        "serve": "npm start --prefix ../",
        "dev": "cross-env NODE_ENV=development electron-forge start",
        "wrapper": "node dev.js",
        "clean": "node build/script.clean.js",
        "build": "node build/script.build.js"
    },
    "keywords": [],
    "author": "luigi",
    "license": "MIT",
    "config": {
        "forge": {
            "make_targets": {
                "win32": [
                    "squirrel"
                ],
                "darwin": [
                    "zip"
                ],
                "linux": [
                    "deb",
                    "rpm"
                ]
            },
            "electronPackagerConfig": {
                "packageManager": "npm"
            },
            "electronWinstallerConfig": {
                "name": "terminal_electron"
            },
            "electronInstallerDebian": {},
            "electronInstallerRedhat": {},
            "github_repository": {
                "owner": "",
                "name": ""
            },
            "windowsStoreConfig": {
                "packageName": "",
                "name": "terminalelectron"
            }
        }
    },
    "dependencies": {
        "electron-compile": "^6.4.2",
        "electron-squirrel-startup": "^1.0.0",
        "load-json-file": "^2.0.0"
    },
    "devDependencies": {
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "electron-forge": "^4.1.3",
        "electron-prebuilt-compile": "1.7.9",
        "eslint": "^3",
        "eslint-config-airbnb": "^15",
        "eslint-plugin-import": "^2",
        "eslint-plugin-jsx-a11y": "^5",
        "eslint-plugin-react": "^7",
        "colors": "^1.1.2",
        "cross-env": "^3.1.3",
        "devtron": "^1.4.0",
        "electron": "^1.5.0",
        "electron-devtools-installer": "^2.0.1",
        "electron-packager": "^8.2.0",
        "eslint-friendly-formatter": "^2.0.7",
        "eslint-loader": "^1.6.2",
        "json-loader": "^0.5.4",
        "node-loader": "^0.6.0",
        "shelljs": "^0.7.5",
        "webpack": "^2.2.1",
        "webpack-merge": "^3.0.0"
    }
}