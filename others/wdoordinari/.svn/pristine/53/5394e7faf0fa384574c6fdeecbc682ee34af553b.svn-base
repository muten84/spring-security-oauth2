/* Menu laterale */

#menu {
  left: 0;
  top: 0;
  bottom: 0;
  background-color: #041c28;
  /*height: 100vh;*/
  overflow: auto;
}

#menu nav {
  padding-bottom: 0px;
  height: 100%;
  overflow: auto;
  height: 100%;
}

#menu nav li {
  padding: 0;
  margin: 0;
  border-top: 2px solid #041c28;
}

#menu nav li:first-child {
  border-top-width: 0;
}

#menu nav li li {
  border-top-width: 1px;
}

