  border-top-width: 1px;
}

#menu nav button,
#menu nav a {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  border-left: 4px solid transparent;
  border-right: 2px solid #041c28;
  padding: 0;
  background-color: #072c3f;
  color: white;
  height: 60px;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  font-size: 1.0em;
}

#menu nav button:focus,
#menu nav button:hover,
#menu nav a:focus,
#menu nav a:hover {
  background-color: #03161f;
}

#menu nav button:focus .nav-icon,
#menu nav button:hover .nav-icon,
#menu nav a:focus .nav-icon,
#menu nav a:hover .nav-icon {
  background-color: #1c7183;
}

#menu nav button.current,
#menu nav button:active,
#menu nav a.current,
#menu nav a:active {
  border-left-color: #00809d;
}

#menu nav button.current .nav-icon,
#menu nav button:active .nav-icon,
#menu nav a.current .nav-icon,
#menu nav a:active .nav-icon {
  background-color: #00809d;
}

