<md-dialog id="resetPassword">
    <form ng-submit="vm.resetPassword()" name="vm.resetPasswordForm" novalidate>
        <md-dialog-content>
            <div class="md-dialog-content">
                <p translate>TYPE_EMAIL_WE_SEND_PASSWORD</p>
                <formly-form model="vm.resetPasswordModel" fields="vm.resetPasswordFields" form="vm.resetPasswordForm">

                </formly-form>

            </div>
        </md-dialog-content>
        <md-dialog-actions>
            <md-button ng-click="vm.closeResetPasswordPopup()">{{'CANCEL' | translate}}</md-button>
            <md-button type="submit" class="md-primary">{{'RECOVER' | translate}}</md-button>
        </md-dialog-actions>
    </form>
</md-dialog>