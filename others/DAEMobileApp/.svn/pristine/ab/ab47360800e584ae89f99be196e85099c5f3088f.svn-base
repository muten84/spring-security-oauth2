#resetPassword{
  md-dialog-actions{
    .md-button{
      font-size: 120%;
    }
  }
  .md-dialog-content{
    padding-bottom: 0;
    p {
      margin-bottom: 10px;
      font-size: 130%;
    }
    md-input-container {
      margin: 18px 0 0 0;
      &.md-input-focused,
      &.md-input-has-value {
        label:not(.md-no-float) {
          transform: translate3d(0, 6px, 0) scale(1);
          font-size: 18px;
          @media (max-width: 320px) {
            font-size: 16px;
          }
        }
      }
      .md-input{
        font-size: 18px;
        @media (max-width: 320px) {
          font-size: 16px;
        }
      }
      &:not(.md-input-focused) {
        &:not(.md-input-invalid) {
          label {
            color: $color_primary;
            font-size: 18px;
            @media (max-width: 320px) {
              font-size: 16px;
            }
            &.md-required:after{
              color: $color_primary;
            }
          }
        }
        &.md-input-invalid {
          label {
            font-size: 18px;
            @media (max-width: 320px) {
              font-size: 16px;
            }
          }
        }
        &.md-input-has-placeholder{
          margin: 36px 0 0 0;
        }
      }
      &.md-input-focused {
        label {
          color: $color_primary;
          &.md-required:after{
            color: $color_primary;
          }
        }
        &.md-input-invalid{
          label {
            font-size: 18px;
            @media (max-width: 320px) {
              font-size: 16px;
            }
            color: rgb(221, 44, 0);
            &.md-required:after{
              color: rgb(221, 44, 0);
            }
          }
        }
      }
    }
  }
}

#exitApp{
  .md-dialog-content{
    padding-bottom: 0;
