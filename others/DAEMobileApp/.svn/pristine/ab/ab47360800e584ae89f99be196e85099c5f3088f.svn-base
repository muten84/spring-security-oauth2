#addAvailability{
  min-width: 300px;
  .md-dialog-content {
    padding-bottom: 0;
    &.error-messages {
      font-size: 14px;
      color: $color_accent;
    }
  }
  md-dialog-actions{
    .md-button{
      font-size: 120%;
      &:not([disabled]):hover{
        background-color: transparent;
      }
    }
  }
  .availability {
    .md-subheader {
      font-size: 18px;
    }
    .repeatsection {
      .formly-field {
        margin: 0;
      }
      md-icon {
        font-size: 20px;
        @media (max-width: 320px) {
          font-size: 16px;
        }
      }
    }
    .clear-btn {
      font-size: 24px;
    }

    .add-new {
      border: 1px solid rgba(0, 0, 0, 0.12);
      height: 40px;
      position: relative;
      .add-icon {
        text-align: center;
        i {
          font-size: 20px;
          line-height: 38px;
        }
      }
      .add-text {
        line-height: 38px;
        font-size: 18px;
        // iphone < 6
        @media (max-width: 320px) {
          font-size: 14px;
        }
      }


