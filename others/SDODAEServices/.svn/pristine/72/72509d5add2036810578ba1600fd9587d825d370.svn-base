package it.eng.areas.ems.sdodaeservices.entity;

import java.util.Calendar;

public enum GiornoSettimanaEnum {

	LUNEDI(Calendar.MONDAY), MARTEDI(Calendar.TUESDAY), MERCOLEDI(Calendar.WEDNESDAY), GIOVEDI(
			Calendar.THURSDAY), VENERDI(Calendar.FRIDAY), SABATO(Calendar.SATURDAY), DOMENICA(Calendar.SUNDAY);

	private Integer order;

	private GiornoSettimanaEnum(Integer order) {
		this.order = order;
	}

	public Integer getOrder() {
		return order;
	}

	public void setOrder(Integer order) {
		this.order = order;
	}

}
