package it.eng.areas.ems.sdodaeservices.delegate.model;

import java.io.Serializable;

import io.swagger.annotations.ApiModel;

@ApiModel(value = "CredentialsBean", description = "Bean utilizzato per l'autenticazione utente")
public class CredentialsBean implements Serializable {

	private String username;

	private String password;
	
	private String passwordHash;

	private Dispositivo deviceInfo;

	public CredentialsBean() {
		// TODO Auto-generated constructor stub
	}

	public String getUsername() {
		return username;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public Dispositivo getDeviceInfo() {
		return deviceInfo;
	}

	public void setDeviceInfo(Dispositivo deviceInfo) {
		this.deviceInfo = deviceInfo;
	}

	public String getPasswordHash() {
		return passwordHash;
	}

	public void setPasswordHash(String passwordHash) {
		this.passwordHash = passwordHash;
	}
	
	
	
	

}
