package it.eng.areas.ems.sdodaeservices.entity;
import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;



@Entity
@Table(name = "DAE_SMTP_PROTOCOL_CONFIG")
public class SMTPProtocolConfigDO implements Serializable {

   @Id
   @Column(name= "ID")
	private String id;

	private Integer numeroPorta;

	private String host;

	private Boolean enableAuthentication;

	private String encryption;

	private String username;

	private String password;

	private String smtpDa;

	private String smtpNomeDa;

	private String smtpRispondiA;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public int getNumeroPorta() {
		return numeroPorta;
	}

	public void setNumeroPorta(int numeroPorta) {
		this.numeroPorta = numeroPorta;
	}

	public String getHost() {
		return host;
	}

	public void setHost(String host) {
		this.host = host;
	}

	public Boolean getEnableAuthentication() {
		return enableAuthentication;
	}

	public void setEnableAuthentication(Boolean enableAuthentication) {
		this.enableAuthentication = enableAuthentication;
	}

	public String getEncryption() {
		return encryption;
	}

	public void setEncryption(String encryption) {
		this.encryption = encryption;
	}

	public String getUsername() {
		return username;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getSmtpDa() {
		return smtpDa;
	}

	public void setSmtpDa(String smtpDa) {
		this.smtpDa = smtpDa;
	}

	public String getSmtpNomeDa() {
		return smtpNomeDa;
	}

	public void setSmtpNomeDa(String smtpNomeDa) {
		this.smtpNomeDa = smtpNomeDa;
	}

	public String getSmtpRispondiA() {
		return smtpRispondiA;
	}

	public void setSmtpRispondiA(String smtpRispondiA) {
		this.smtpRispondiA = smtpRispondiA;
	}

	public SMTPProtocolConfigDO() {
		super();
		// TODO Auto-generated constructor stub
	}

	
}
