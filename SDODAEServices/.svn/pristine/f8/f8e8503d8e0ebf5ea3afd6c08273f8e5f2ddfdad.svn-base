        <li id="nmberDAEandFR" areas-dashboard-entry ng-controller="NmberDAEandFRCtrl" class=" dashboard-entry panel-container double">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <div class="panel-id">
                        <span class="panel-icon">
                            <i class="fa fa-bar-chart"></i>
                        </span>
                        <h2 class="panel-title">Numero totale DAE e DAE Responder</h2>
                    </div>
                </div>
                <div class="panel-body" style="height: 325px; width: 100%">


                    <div class="row">
                        <div class="col-md-8">
                            <h3>DAE</h3>
                        </div>
                        
                    </div>
                    <div class="row" ng-repeat="(key, value) in val.dae">
                        <div class="col-md-8">
                            <h5>{{key}}</h5>
                        </div>
                        <div class="col-md-4">
                            <h5>{{value}}</h5>
                        </div>
          
                    </div>

                    <div class="row" style="border-bottom: 1px black solid">
                        <div class="col-md-8">
                            <label>DAE Totali</label>
                        </div>
                        <div class="col-md-4">
                            <label>{{val.totalDAE}}</label>
                        </div>
                    </div>

                    <div class="row" >
                        <div class="col-md-8">
                            <h3>DAE Responder</h3>
                        </div>
                         
                    </div>
                    <div class="row" ng-repeat="(key, value) in val.fr">
                        <div class="col-md-8">
                            <h5>{{key}}</h5>
                        </div>
                        <div class="col-md-4">
                            <h5>{{value}}</h5>
                        </div>
 
                    </div>

                    <div class="row">
                        <div class="col-md-8">
                            <label>DAE Responder Totali</label>
                        </div>
                        <div class="col-md-4">
                            <label>{{val.totalFR}}</label>
                        </div>
                    </div>

                    <!-- <canvas class="chart chart-bar" chart-data="data" chart-labels="labels" chart-series="series" chart-options="options"></canvas> -->
                </div>
            </div>
        </li>

