package it.eng.areas.ems.sdodaeservices.entity;
import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;



@Entity
@Table(name = "DAE_NOTA_DAE")
public class NotaDAEDO implements Serializable {

	   @Id
	   @Column(name= "ID")
	private String id;

	private String dae;

	private GPSLocationDO location;

	private String descrizione;

	
	public NotaDAEDO() {
		super();
		// TODO Auto-generated constructor stub
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getIdDAE() {
		return dae;
	}

	public void setIdDAE(String idDAE) {
		dae = dae;
	}

	public GPSLocationDO getLocation() {
		return location;
	}

	public void setLocation(GPSLocationDO location) {
		this.location = location;
	}

	public String getDescrizione() {
		return descrizione;
	}

	public void setDescrizione(String descrizione) {
		descrizione = descrizione;
	}

	
	
}
																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																							